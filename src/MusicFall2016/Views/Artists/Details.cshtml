@model IEnumerable<MusicFall2016.Models.Artist>
@{
    ViewData["Title"] = "Artists";
}
<h2>Details</h2>
<form action="Create" method="get">
    <button>Create Artist</button>
</form>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Bio</th>
        </tr>
    </thead>
    @foreach (var artist in Model)
    {
        <tr>
            <td>@artist.Name</td>
            <td>@artist.Bio</td>

            <td>
                <a asp-action="Update" asp-route-id="@artist.ArtistID">Edit</a>
                <a asp-action="Retrieve" asp-route-id="@artist.ArtistID">Details</a>

          
            </td>
        </tr>


    }
</table>


